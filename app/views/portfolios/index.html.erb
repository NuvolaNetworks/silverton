<h1>Listing portfolios</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Image</th>
      <th>Image2</th>
      <th>Image3</th>
      <th>Image4</th>
      <th>Image6</th>
      <th>Image8</th>
      <th>Image9</th>
      <th>Image10</th>
      <th>Image11</th>
      <th>Image12</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @portfolios.each do |portfolio| %>
      <tr>
        <td><%= portfolio.title %></td>
        <td><%= portfolio.description %></td>
        <td><%= portfolio.image %></td>
        <td><%= portfolio.image2 %></td>
        <td><%= portfolio.image3 %></td>
        <td><%= portfolio.image4 %></td>
        <td><%= portfolio.image6 %></td>
        <td><%= portfolio.image8 %></td>
        <td><%= portfolio.image9 %></td>
        <td><%= portfolio.image10 %></td>
        <td><%= portfolio.image11 %></td>
        <td><%= portfolio.image12 %></td>
        <td><%= link_to 'Show', portfolio %></td>
        <td><%= link_to 'Edit', edit_portfolio_path(portfolio) %></td>
        <td><%= link_to 'Destroy', portfolio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Portfolio', new_portfolio_path %>
